{
  "cli": {
    "version": ">= 13.1.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": false,
      "autoIncrement": "true",
      "android": {
        "image": "latest",
        "distribution": "internal"
      },
      "ios": {
        "distribution": "store"
      }
    },
    "internal_test": {
      "developmentClient": false,
      "distribution": "internal",
      "autoIncrement": "true",
      "android": {
        "image": "latest",
        "buildType": "app-bundle"
      }
    },
    "test_flight": {
      "developmentClient": false,
      "distribution": "store",
      "autoIncrement": "true"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}
